{
    "name": "bohnmedia/contao-pretix-bundle",
    "description": "Contao bundle to integrate pretix on you website.",
    "keywords": [
      "contao",
      "pretix",
      "tickets",
      "events"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/bohnmedia/contao-pretix-bundle",
    "authors": [
      {
        "name": "Dennis Bohn",
        "homepage": "https://www.bohnmedia.de",
        "role": "Developer"
      }
    ],
    "support": {
      "email": "dennis.bohn@bohnmedia.de"
    },
    "require": {
      "php": ">=8.1",
      "contao/core-bundle": "^5.2"
    },
    "require-dev": {
      "contao/manager-plugin": "^2.0"
    },
    "conflict": {
      "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
      "psr-4": {
        "BohnMedia\\ContaoPretixBundle\\": "src"
      }
    },
    "extra": {
      "contao-manager-plugin": "BohnMedia\\ContaoPretixBundle\\ContaoManager\\Plugin"
    },
    "config": {
      "allow-plugins": {
        "contao/manager-plugin": true,
        "contao-components/installer": true
      }
    }
  }